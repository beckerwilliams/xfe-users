{
  "servers": {
    "app": {
      "options": {
        "host": "localhost",
        "port": 3000
      }
    },
    "admin": {
      "options": {
        "host": "localhost",
        "port": 3737
      }
    }
  },
  "cors_options": {
    "methods": "GET,PUT,DELETE,POST,HEAD,OPTIONS",
    "origin": "*"
  },
  "scan": {
    "default_file_exclusions": [
      ".keepme",
      ".pyenv.d",
      "node_modules",
      "Library",
      ".deps",
      ".git",
      "node",
      ".gitignore",
      "tmp",
      "work",
      "working"
    ],
    "default_file_extensions_rgx": "\\.pem$|\\.der$|\\.cer$|\\.key$|\\.pub$|\\.png$",
    "default_file_excludes_rgx": "\\.deps$|\\.git$|\\.gitignore|\\.pyenv.d$|node_modules|Library|tmp|work|working"
  }
}